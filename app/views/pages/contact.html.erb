<div class="container">
	<h3>Contact</h3>
	<div class="card z-depth-2">
		<div class="card-content">
			<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC0IY0NH3EXwX-B_pNLBUnIVq6pA6ocDLo&callback=initMap"
							async defer></script>
			<script>
				var map;
				var myPos = {lat: 47.604061, lng: -122.3212237};
				function initMap() {
					map = new google.maps.Map(document.getElementById('map'), {
						center: myPos,
						zoom: 15,
					});
					var marker = new google.maps.Marker({
						position:myPos,
						title: "Home at last :)"
					});
					marker.setMap(map);
				}
			</script>
			<div id="map" style="height:500px; width:100%;"></div>

<!--			<iframe style="width:100%;" height="450" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/view?zoom=10&center=47.6062%2C-122.3321&key=AIzaSyC0IY0NH3EXwX-B_pNLBUnIVq6pA6ocDLo" allowfullscreen></iframe>-->
		</div>
	</div>
</div>
